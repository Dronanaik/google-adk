# Google ADK Configuration for Streamlit Code Generator Agent

# Option 1: Using Google AI Studio (Recommended for getting started)
# Get your API key from: https://aistudio.google.com/apikey
GOOGLE_GENAI_USE_VERTEXAI=FALSE
GOOGLE_API_KEY=YOUR_API_KEY_HERE

# Option 2: Using Google Cloud Vertex AI
# Uncomment the lines below and comment out the lines above
# GOOGLE_GENAI_USE_VERTEXAI=TRUE
# GOOGLE_CLOUD_PROJECT=YOUR_PROJECT_ID
# GOOGLE_CLOUD_LOCATION=us-central1

# Option 3: Using Vertex AI Express Mode (Free tier)
# Uncomment the lines below and comment out the lines above
# GOOGLE_GENAI_USE_VERTEXAI=TRUE
# GOOGLE_API_KEY=YOUR_EXPRESS_MODE_API_KEY_HERE